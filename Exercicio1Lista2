        <body>
            <div class="fundo">
            <h1>Primeiros passos JavaScript</h1>
            <script>

            var numero = prompt("Digite um número:");
            numero = parseFloat(numero);

            var indice = 10;
            while( indice<=100 )
            {
                
            document.write(numero + ' x ' + indice + ' = ' + (numero*indice) + '<hr>');
            indice = indice + 10;
            }

    
</script>
        
    </div>
</body>
